<% doc_presenter = index_presenter(document) %>
<%# default partial to display solr document fields in catalog index view -%>
<dl class="document-metadata dl-invert row">
  <div class="document-thumbnail">
    <img src="https://curate-qa.curationexperts.com/downloads/423612jm7g-cor?file=thumbnail" alt="">
  </div>
  <div class="document-metadata">
    <% doc_presenter.fields_to_render.each do |field_name, field| -%>
      <dt class="blacklight-<%= field_name.parameterize %> col-md-9"><%= render_index_field_label document, field: field_name %></dt>
      <dd class="blacklight-<%= field_name.parameterize %> col-md-9"><%= doc_presenter.field_value field %></dd>
    <% end -%>
  </div>
</dl>
